    "build-tvnamer": "python setup.py install",
    "start": "node server.js",
    "prettier": "prettier --write '{,!(node_modules)/**/}*.js'"
  },
  "dependencies": {
    "axios": "^0.18.0",
    "base64-js": "1.3.x",
    "execSync": "1.0.x",
